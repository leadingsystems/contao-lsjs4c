{
  "name": "leadingsystems/contao-lsjs4c",
  "description": "Leading Systems LSJS4C as a Contao 4 bundle",
  "keywords": [
    "contao",
    "leading systems",
    "bundle"
  ],
  "type": "contao-bundle",
  "license": "proprietary",
  "require": {
    "php": "^7.0.0 || ^8.0.0",
    "contao/core-bundle": "~4.9",
    "leadingsystems/contao-helpers": "^2.0.0",
    "leadingsystems/lsjs": "^3.0.0"
  },
  "autoload": {
    "psr-4": {
      "LeadingSystems\\LSJS4CBundle\\": "src/"
    },
    "classmap": [
      "src/Resources/contao/"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config/",
      "src/Resources/contao/dca/",
      "src/Resources/contao/languages/",
      "src/Resources/contao/templates/"
    ]
  },
  "extra": {
    "contao-manager-plugin": "LeadingSystems\\LSJS4CBundle\\ContaoManager\\Plugin",
    "branch-alias": {
      "dev-master": "3.0.x-dev"
    }
  }
}
