{
  "name": "leadingsystems/contao-lsjs4c",
  "description": "Leading Systems LSJS4C as a Contao 4 bundle",
  "keywords": [
    "contao",
    "leading systems",
    "bundle"
  ],
  "type": "contao-bundle",
  "require": {
    "php": ">=7.0.0",
    "contao/core-bundle": "~4.4",
    "leadingsystems/contao-helpers": "*",
    "leadingsystems/lsjs": "*"
  },
  "autoload": {
    "psr-4": {
      "LeadingSystems\\LSJS4CBundle\\": "src/"
    },
    "classmap": [
      "src/Resources/contao/"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config/",
      "src/Resources/contao/dca/",
      "src/Resources/contao/languages/",
      "src/Resources/contao/templates/"
    ]
  },
  "extra": {
    "contao-manager-plugin": "LeadingSystems\\LSJS4CBundle\\ContaoManager\\Plugin"
  }
}